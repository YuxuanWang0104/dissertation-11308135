   "source": [
    "# 已知值\n",
    "x1 <- 0.2\n",
    "x4 <- 0.25\n",
    "\n",
    "# 计算公比 r\n",
    "r <- (x4 / x1)^(1/3)\n",
    "\n",
    "# 初始化等比数列\n",
    "geo_sequence <- numeric(10)\n",
    "\n",
    "# 计算等比数列的每一项，并保留小数点后7位\n",
    "for (i in 1:10) {\n",
    "  geo_sequence[i] <- round(x1 * r^(i - 1), 4)\n",
    "}\n",
    "\n",
    "# 输出公比和等比数列\n",
    "r <- round(r, 7)\n",
    "r\n",
    "geo_sequence\n"
   ]
